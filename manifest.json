{
  "manifest_version": 2,
  "name": "AdBlock for Youtube",
  "version": "0.1",
  "description": "Removes ads from Youtubeâ„¢.\n",
  "permissions": [
    "storage",
    "scripting"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "https://raw.githubusercontent.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["userscript.js"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}